{
  "name": "latexis",
  "displayName": "LaTeXiS Tools",
  "publisher": "LuisRobles",
  "author": {
    "name": "Luis Robles"
  },
  "icon": "images/icon.png",
  "description": "Advanced LaTeX extension for VS Code focused on automating and simplifying academic and scientific document creation. LaTeXiS assists thesis writers and researchers with intelligent tooling such as automated table generation, LaTeX package management, document analysis, and extensible snippets for articles, theses, books, and institutional templates.",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/rralDev/latexis.git"
  },
  "engines": {
    "vscode": "^1.106.1"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters"
  ],
  "keywords": [
    "latex",
    "LaTeX",
    "latex-tools",
    "latex-templates",
    "latex-tables",
    "excel-to-latex",
    "xlsx",
    "latex-table-generator",
    "booktabs",
    "multirow",
    "tabular",
    "tabularx",
    "biblatex",
    "biber",
    "apa",
    "latex-workflow",
    "academic-writing",
    "thesis",
    "theses",
    "tesis",
    "research",
    "scientific-writing",
    "vscode-latex",
    "latex-extension",
    "clipboard",
    "tsv",
    "excel",
    "tables",
    "automation"
  ],
  "activationEvents": [
    "onCommand:latexis.manageReferences",
    "onCommand:latexis.insertFigure",
    "onCommand:latexis.manageTables",
    "onCommand:latexis.insertEquation",
    "onCommand:latexis.scanDocument"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "latexis.manageReferences",
        "title": "LaTeXiS: Gestionar referencias"
      },
      {
        "command": "latexis.insertFigure",
        "title": "LaTeXiS: Insertar figura"
      },
      {
        "command": "latexis.manageTables",
        "title": "LaTeXiS: Insertar tabla"
      },
      {
        "command": "latexis.insertEquation",
        "title": "LaTeXiS: Insertar ecuación"
      },
      {
        "command": "latexis.scanDocument",
        "title": "LaTeXiS: Analizar documento y añadir paquetes faltantes"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.106.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.1",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "xlsx-js-style": "^1.2.0"
  }
}
