    "LaTeX",
    "latex-tools",
    "latex-templates",
    "latex-tables",
    "excel-to-latex",
    "xlsx",
    "latex-table-generator",
    "booktabs",
    "multirow",
    "tabular",
    "tabularx",
    "biblatex",
    "biber",
    "apa",
    "latex-workflow",
    "academic-writing",
    "thesis",
    "theses",
    "tesis",
    "research",
    "scientific-writing",
    "vscode-latex",
    "latex-extension",
    "clipboard",
    "tsv",
    "excel",
    "tables",
    "automation"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "latexis.helloWorld",
        "title": "LaTeXiS: Hello World (debug)"
      },
      {
        "command": "latexis.insertFigure",
        "title": "LaTeXiS: Insertar figura"
      },
      {
        "command": "latexis.insertEquation",
        "title": "LaTeXiS: Insertar ecuaci칩n"
      },
      {
        "command": "latexis.scanDocument",
        "title": "LaTeXiS: Analizar documento y a침adir paquetes faltantes"
      },
      {
        "command": "latexis.insertAPAConfig",
        "title": "LaTeXiS: Insertar configuraci칩n APA (biblatex + biber)"
      },
      {
        "command": "latexis.pasteSimpleTable",
        "title": "LaTeXiS: Pegar tabla desde portapapeles"
      },
      {
        "command": "latexis.insertExcelTable",
        "title": "LaTeXiS: Insertar tabla desde archivo Excel"
      },
      {
        "command": "latexis.changeCitationStyle",
        "title": "LaTeXiS: Cambiar estilo de citaci칩n (BibLaTeX)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.106.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.1",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "xlsx-js-style": "^1.2.0"
